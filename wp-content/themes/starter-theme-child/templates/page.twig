{% extends "base.twig" %}

{% block content %}
	<div class="row">

		<article class="post-type post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<h1 class="margin-xl">{{post.title}}</h1>
			<div class="article-body">
				{{ post.content }}
			</div>
		</article>

	</div>
{% endblock %}

{% block contact %}{% endblock %}