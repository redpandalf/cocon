{% if menu %}
	<ul>
	{% for item in items %}
		<li class="{{ item.classes | join(' ') }}">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% include "menu.twig" with {'items': item.children} %}<i class="fa-sharp fa-solid fa-envelope"></i>
		</li>
	{% endfor %}
	</ul>
{% endif %}
